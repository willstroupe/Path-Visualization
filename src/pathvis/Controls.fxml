<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<HBox spacing="2.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pathvis.Controls">
   <children>
      <Button fx:id="startButton" mnemonicParsing="false" onMouseClicked="#startButtonClicked" text="Start" />
      <Button fx:id="stepButton" mnemonicParsing="false" onMouseClicked="#stepButtonClicked" text="Step" />
      <Button fx:id="stopButton" mnemonicParsing="false" onMouseClicked="#stopButtonClicked" text="Stop" />
      <Label prefHeight="24.0" prefWidth="136.0" text="Time Between Steps (ms):" />
      <TextField fx:id="stepTimeField" prefHeight="25.0" prefWidth="63.0" text="10" />
      <Label prefHeight="24.0" fx:id="infoLabel" />
   </children>
   <padding>
      <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
   </padding>
</HBox>
